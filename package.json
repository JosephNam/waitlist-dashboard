{
  "name": "WaitlistDashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --max_old_space_size=4096 ./server/bin/www",
    "start:watch": "nodemon --max_old_space_size=4096 --prof ./server/bin/www",
    "build": "pipeline/build_app",
    "build:docker": "pipeline/build_docker",
    "build:watch": "webpack --progress --colors --watch",
    "test": "nyc ava && nyc report --reporter=html",
    "test:watch": "npm test -- --watch",
    "deploy": "pipeline/deploy_latest",
    "selenium": "./node_modules/selenium-standalone/bin/selenium-standalone install && ./node_modules/selenium-standalone/bin/selenium-standalone start",
    "nightwatch": "./node_modules/nightwatch/bin/nightwatch"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "app-root-path": "^1.2.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-helpers": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-messages": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.9.2",
    "babel-traverse": "^6.10.4",
    "babel-types": "^6.11.1",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "browserify": "^13.0.1",
    "compression": "^1.6.2",
    "convert-source-map": "^1.2.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "ejs": "^2.4.2",
    "es6-promise": "^3.2.1",
    "eslint": "^3.1.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.14.0",
    "material-ui": "^0.15.2",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "nyc": "^7.0.0",
    "ot-discovery": "^0.10.2",
    "ot-logger": "^0.6.1",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-date-picker": "^5.3.26",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "request": "^2.73.0",
    "request-promise": "^4.1.0",
    "serve-favicon": "^2.3.0",
    "superagent": "^2.1.0",
    "superagent-es6-promise": "^1.0.0",
    "victory": "^0.9.0",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "ava": "*",
    "babel-core": "^6.11.4",
    "babel-eslint": "*",
    "babel-plugin-espower": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-register": "^6.11.6",
    "babelify": "*",
    "browserify": "*",
    "chai": "*",
    "enzyme": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "immutable": "*",
    "jsx-test-helpers": "*",
    "nightwatch": "*",
    "nyc": "*",
    "react-hot-loader": "*",
    "redux-thunk": "^2.1.0",
    "selenium-download": "*",
    "selenium-standalone": "*",
    "selenium-webdriver": "*",
    "superagent-es6-promise": "*",
    "watchify": "*"
  },
  "description": "visualizing waitlist prediction times and mixpanel funnels",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephNam/waitlist-dashboard.git"
  },
  "keywords": [],
