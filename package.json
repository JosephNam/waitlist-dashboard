{
  "name": "WaitlistDashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/bin/www",
    "build": "bin/build.sh",
    "docker:build": "bin/docker-build.sh",
    "build:watch": "echo $PWD && start watchify -v -t [ babelify --presets [es2015 react] ] ./client/src/*.js -o ./client/bundle.js",
    "test": "nyc ava && nyc report --reporter=html",
    "test:watch": "npm test -- --watch"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "app-root-path": "^1.2.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "babelify": "^7.3.0",
    "body-parser": "~1.13.2",
    "browserify": "^13.0.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "^2.3.4",
    "es6-promise": "^3.2.1",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "ot-discovery": "^0.10.2",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "serve-favicon": "~2.3.0",
    "superagent": "^2.0.0",
    "victory": "^0.9.0",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.0",
    "eslint-plugin-react": "^5.1.1",
    "immutable": "^3.8.1",
    "jsx-test-helpers": "^1.1.0",
    "nyc": "^6.6.1",
    "superagent-es6-promise": "git+https://git@github.com/opentable/superagent-es6-promise.git",
    "watchify": "^3.7.0"
  },
  "description": "visualizing waitlist prediction times and mixpanel funnels",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephNam/waitlist-dashboard.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JosephNam/waitlist-dashboard/issues"
  },
  "homepage": "https://github.com/JosephNam/waitlist-dashboard#readme"
}
